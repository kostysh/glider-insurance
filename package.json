  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/windingtree/glider-insurance/issues"
  },
  "homepage": "https://github.com/windingtree/glider-insurance#readme",
  "engines": {
    "node": ">=12.0.0"
  },
  "devDependencies": {
    "dotenv": "8.2.0",
    "vercel": "21.0.1",
    "eslint": "7.14.0",
    "mocha": "8.2.1",
    "chai": "4.2.0",
    "nyc": "15.1.0",
    "newman": "5.2.1"
